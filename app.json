{
  "expo": {
    "name": "Simple Math for Everyone, Quiz, Test",
    "slug": "simplemathforeveryone",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#03a9f4"
    },
    "updates": {
      "fallbackToCacheTimeout": 30000,
      "enabled": true,
      "checkAutomatically": "ON_LOAD"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jvmastery.simplemathexercise"
    },
    "android": {
      "package": "com.jvmastery.simplemathexercise"
    },
    "description": "Test your math skills! All ages, everyone can play!\nBasic mathematical operations as addition, subtraction, multiplication, division. With random numbers in your range and no time limit!"
  }
}
